{
    "training":{
      "arch_type": "epi_slides",
      "n_epochs": 50,
      "save_epoch_freq": 1,
      "lr_policy": "one_cycle",
      "lr_decay_iters": 250,
      "batchSize": 2,
      "preloadData": false,
      "split_pct":0.8
    },
    "visualisation":{
      "display_port":9898,
      "no_html": true,
      "display_winsize": 256,
      "display_id": 0,
      "lim":15,
      "run_name":"TransUnet",
      "use_wandb": true,
      "display_single_pane_ncols": 0
    },
    "data_path": {
    "epi": "kaggle/input/stain-normalisation/SN",
    "epi_slides": "kaggle/input/stain-normalisation/SN"
  
    },
    "augmentation": {
      "epi": {
        "shift": [0.1,0.1],
        "rotate": 15.0,
        "scale": [0.7,1.3],
        "intensity": [1.0,1.0],
        "random_flip_prob": 0.5,
        "scale_size": 256,
        "patch_size": [256,256]
      }, 
      "epi_slides": {
        "shift": [0.1,0.1],
        "rotate": 15.0,
        "scale": [0.7,1.3],
        "intensity": [1.0,1.0],
        "random_flip_prob": 0.5,
        "scale_size": 256,
        "patch_size": [256,256]
      },
      "test": {
        "shift": [0.1,0.1],
        "rotate": 15.0,
        "scale": [0.7,1.3],
        "intensity": [1.0,1.0],
        "random_flip_prob": 0.5,
        "scale_size": 256,
        "patch_size": [256,256]
      }
    },
    "model":{
      "type":"seg",
      "continue_train":false,
      "which_epoch": -1,
      "model_type": "R50-ViT-B_16",
      "model_types": ["ViT-B_32","ViT-L_32","ViT-L_16","ViT-H_14","R50-ViT-B_32","R50-ViT-B_16"],
      "tensor_dim": "2D",
      "division_factor": 1000,
      "img_size": 256,
      "input_nc": 3,
      "output_nc": 3,
      "lr_rate": 1e-4,
      "max_lr":1e-2,
      "feature_scale": 4,
      "gpu_ids": [0],
      "isTrain": true,
      "checkpoints_dir": "./checkpoints",
      "experiment_name": "experiment_TransUnet",
      "criterion": "iou_loss",
      "nonlocal_mode":"concatenation_residual",
      "optim":"adam"
    }
  }
  